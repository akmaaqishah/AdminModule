<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/headerwaves1"
        android:layout_width="419dp"
        android:layout_height="260dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-8dp"
        android:layout_marginTop="-54dp"
        android:layout_marginEnd="0dp"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/waves" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/headerwaves2"
        android:layout_width="422dp"
        android:layout_height="255dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-11dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="-53dp"
        android:scaleY="-1
"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/waves" />

    <TextView
        android:id="@+id/dasboard"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="39dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="217dp"
        android:layout_marginBottom="502dp"
        android:fontFamily="@font/dm_bold"
        android:text="Dashboard"
        android:textColor="#000000"
        android:textSize="22sp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="229dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="13dp"
        android:layout_marginTop="267dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="235dp"
        android:layout_toEndOf="@+id/imageView3"
        app:srcCompat="@drawable/roundeddashboard" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="34dp"
        android:layout_marginBottom="243dp"
        app:srcCompat="@drawable/roundeddashboard" />

    <TextView
        android:id="@+id/parcel"
        android:layout_width="78dp"
        android:layout_height="28dp"
        android:layout_alignStart="@+id/imageView4"
        android:layout_alignTop="@+id/imageView4"
        android:layout_alignEnd="@+id/imageView4"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_marginStart="41dp"
        android:layout_marginTop="144dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="45dp"
        android:fontFamily="@font/dm_regular"
        android:text="Parcel"
        android:textColor="#000000"
        android:textSize="25sp" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/qrcode"
        android:layout_width="163dp"
        android:layout_height="166dp"
        android:layout_alignStart="@+id/imageView6"
        android:layout_alignTop="@+id/imageView6"
        android:layout_alignEnd="@+id/imageView6"
        android:layout_alignBottom="@+id/imageView6"
        android:layout_marginStart="3dp"
        android:layout_marginTop="-7dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="58dp"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/qrcode" />

    <TextView
        android:id="@+id/scanner"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_alignStart="@+id/imageView6"
        android:layout_alignTop="@+id/imageView6"
        android:layout_alignEnd="@+id/imageView6"
        android:layout_alignBottom="@+id/imageView6"
        android:layout_marginStart="23dp"
        android:layout_marginTop="172dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="15dp"
        android:text="Scanner"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25sp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="160dp"
        android:layout_alignStart="@+id/imageView4"
        android:layout_alignTop="@+id/imageView4"
        android:layout_alignEnd="@+id/imageView4"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_marginStart="-10dp"
        android:layout_marginTop="-5dp"
        android:layout_marginEnd="-4dp"
        android:layout_marginBottom="61dp"
        app:srcCompat="@drawable/parcel" />

</RelativeLayout>